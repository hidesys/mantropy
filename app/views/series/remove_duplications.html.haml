%table
  - @series.each do |serie|
    %tr
      %td
        id:
        = serie.id
      %td= link_to image_tag(serie_image_path(serie, :small), :alt=> serie.name, :style => "float: left;"), serie, {:target => "_blank"}
      %td= link_to serie.name, serie, {:target => "_blank"}
      %td
        - serie.ranks(:ranking_id => 5).each do |rank|
          = rank.ranking.name
          = rank.rank
          位
          = link_to rank.user.name, user_path(rank.user.name) if rank.user
          %br/
      %td
        ここに統合ボタン
