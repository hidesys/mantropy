= form_for(@browsenodeid) do |f|
  - if @browsenodeid.errors.any?
    #error_explanation
      %h2
        = pluralize(@browsenodeid.errors.count, "error")
        prohibited this browsenodeid from being saved:
      %ul
        - @browsenodeid.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :node
    %br/
    = f.text_field :node
  .field
    = f.label :name
    %br/
    = f.text_field :name
  .field
    = f.label :ancestor
    %br/
    = f.text_field :ancestor
  .actions
    = f.submit
