<% @users.each do |user| %>
  <div class="member">
    <%= link_to (image_tag "/images/nil_medium.jpg", :alt => user.name, :width=> 128, :height=> 128), user_path(user.name) %>
    <br />
    <%= link_to (user.name), user_path(user.name) %>
    <br />
    <%= user.publicabout && user.publicabout != "" ? "#{user.publicabout[0..8]}#{user.publicabout.length > 9 ? "…" : nil}" : "頭がァッ！　ぶわわ" %>
  </div>
<% end %>
<br style="clear: both;" />

