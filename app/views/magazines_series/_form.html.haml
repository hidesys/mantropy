= form_for(@magazines_serie) do |f|
  - if @magazines_serie.errors.any?
    #error_explanation
      %h2
        = pluralize(@magazines_serie.errors.count, "error")
        prohibited this magazines_serie from being saved:
      %ul
        - @magazines_serie.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :magazines
    %br/
    = f.text_field :magazines
  .field
    = f.label :series
    %br/
    = f.text_field :series
  .field
    = f.label :placed
    %br/
    = f.text_field :placed
  .actions
    = f.submit
